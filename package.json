{
  "name": "maileeze",
  "version": "0.0.1-alpha",
  "description": "A html5 email parser and optimizer",
  "main": "lib/index.js",
  "bin": {
    "mailee": "node lib/bin/cli.js"
  },
  "scripts": {
    "start": "npx tsnd -P ./tsconfig.json --respawn ./src/index.ts",
    "test": "npx ts-node --project ./tsconfig.json ./src/index.ts compile ./test/index.html ./test/out.html",
    "build": "npx tsc"
  },
  "keywords": [
    "web",
    "scaffold",
    "typescript",
    "node",
    "react"
  ],
  "author": "ChlorineC",
  "license": "MIT",
  "packageManager": "pnpm@8.3.1",
  "devDependencies": {
    "@types/node": "^18.16.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.8.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.1",
    "parse5": "^7.1.2"
  }
}